
public class UnObservador extends Observador{
	
	private double[] _data;
	
	public UnObservador (Sujeto s) {
	super(s);
	_data = new double[4];
	}
	public void update () {
		
	double[] data=new double[4];
	
	data =
	((UnSujeto)_subject).getState();
	
	for (int i=0; i<4; i++)
	_data[i] = data[i];
	this.redraw();
	}
	
	public void redraw () {
		for (int i=0; i<4; i++)
		System.out.println("Hasta ahora"+ _data[i]);		
	}

}
