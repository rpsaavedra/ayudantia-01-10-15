
public class UnObservador extends Observador{
	
	private double _data;
	
	public UnObservador (Sujeto s) {
	super(s);
	_data=0;
	}
	public void update () {
		
	double data;
	
	data =
	((UnSujeto)_subject).getState();
	
	this.redraw();
	}
	
	public void redraw () {
		for (int i=0; i<4; i++)
		System.out.println("Ahora está la factura"+ _data);		
	}

}
